config {
  no_cache = 1
  debug = 0
  xhtml_cleaning = 0
  admPanel = 0
  disableAllHeaderCode = 1
  sendCacheHeaders = 0
  sys_language_uid = 0
  sys_language_mode = ignore
  sys_language_overlay = 1
  additionalHeaders.10.header = Content-Type: text/html; charset=utf-8
  additionalHeaders.10.replace = 1
}

config {
  absRefPrefix = /
  recordLinks.content {
    forceLink = 1
    typolink {
      parameter = 1200
      section {
        data = field:uid
        wrap = c|
      }
    }
  }
}

page = PAGE
page {
    10 = TEXT
    10.typolink.parameter = {$typolink.parameter}
    10.typolink.useCacheHash = 1
}
